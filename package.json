{
  "name": "@kispace-io/appspace",
  "version": "0.0.0",
  "description": "A highly modular and extensible web application framework for building IDE-like applications with a plugin architecture, AI integration, and comprehensive workspace management.",
  "type": "module",
  "license": "EPL-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kispace-io/appspace.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./extensions/ai-system": {
      "types": "./dist/extensions/ai-system/index.d.ts",
      "import": "./dist/extensions/ai-system/index.js"
    },
    "./extensions/ai-system/*": {
      "types": "./dist/extensions/ai-system/*.d.ts",
      "import": "./dist/extensions/ai-system/*.js"
    },
    "./extensions/rag-system": {
      "types": "./dist/extensions/rag-system/index.d.ts",
      "import": "./dist/extensions/rag-system/index.js"
    },
    "./extensions/rag-system/*": {
      "types": "./dist/extensions/rag-system/*.d.ts",
      "import": "./dist/extensions/rag-system/*.js"
    },
    "./extensions/in-browser-ml": {
      "types": "./dist/extensions/in-browser-ml/index.d.ts",
      "import": "./dist/extensions/in-browser-ml/index.js"
    },
    "./extensions/in-browser-ml/*": {
      "types": "./dist/extensions/in-browser-ml/*.d.ts",
      "import": "./dist/extensions/in-browser-ml/*.js"
    },
    "./extensions/github-service/github-service": {
      "types": "./dist/extensions/github-service/github-service.d.ts",
      "import": "./dist/extensions/github-service/github-service.js"
    },
    "./widgets": {
      "types": "./dist/widgets/index.d.ts",
      "import": "./dist/widgets/index.js"
    },
    "./widgets/*": {
      "types": "./dist/widgets/*.d.ts",
      "import": "./dist/widgets/*.js"
    },
    "./parts": {
      "types": "./dist/parts/index.d.ts",
      "import": "./dist/parts/index.js"
    },
    "./parts/*": {
      "types": "./dist/parts/*.d.ts",
      "import": "./dist/parts/*.js"
    },
    "./externals/webawesome": {
      "types": "./dist/externals/webawesome.d.ts",
      "import": "./dist/externals/webawesome.js"
    },
    "./externals/lit": {
      "types": "./dist/externals/lit.d.ts",
      "import": "./dist/externals/lit.js"
    },
    "./externals/third-party": {
      "types": "./dist/externals/third-party.d.ts",
      "import": "./dist/externals/third-party.js"
    }
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@awesome.me/webawesome": "^3.0.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@langchain/textsplitters": "^1.0.0",
    "@lit-labs/signals": "^0.1.3",
    "@mlc-ai/web-llm": "^0.2.79",
    "@xenova/transformers": "^2.17.2",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "idb-keyval": "^6.2.2",
    "jsonata": "^2.1.0",
    "jszip": "^3.10.1",
    "lit": "^3.2.1",
    "marked": "^12.0.0 || ^16.4.1",
    "monaco-editor": "0.55.1",
    "pace-js": "^1.2.4",
    "pdfjs-dist": "^4.0.0",
    "pip-requirements-js": "^1.0.2",
    "pyodide": "^0.27.7",
    "rxdb": "^15.0.0",
    "uuid": "^13.0.0",
    "wasm-git": "^0.0.13"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  }
}
